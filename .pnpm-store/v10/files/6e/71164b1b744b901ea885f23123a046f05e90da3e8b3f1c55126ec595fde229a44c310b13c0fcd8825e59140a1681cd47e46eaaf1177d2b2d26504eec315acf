import { Project, ProjectOptions } from "projen";
import { GeneratedPlantumlDocumentationOptions } from "../../types";
export interface GeneratedPlantumlDocumentationProjectOptions extends ProjectOptions, GeneratedPlantumlDocumentationOptions {
    /**
     * Path to the OpenAPI Specification for which to generate docs, relative to the project outdir
     */
    readonly specPath: string;
}
export declare class GeneratedPlantumlDocumentationProject extends Project {
    private readonly generateTask;
    constructor(options: GeneratedPlantumlDocumentationProjectOptions);
}
